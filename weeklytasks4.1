def multiply_list_elements(numbers):
    """
    Multiplies all number elements in the list. Ignores non-numeric elements.
    """
    # Initialize the result as 1 (multiplicative identity)
    result = 1

    # Iterate over each element in the list
    for element in numbers:
        # Check if the element is a number (int or float)
        if isinstance(element, (int, float)):
            result *= element

    return result

def main():
    """
    Main function to demonstrate the multiplication of list elements.
    """
    print("List Element Multiplier")

    # Example input list
    input_list = [1, 2, 3, 'a', 4, "text"]
    print("Input list:", input_list)

    # Calculate the product
    product = multiply_list_elements(input_list)
    print("Product of all numeric elements:", product)

if __name__ == "__main__":
    main()
